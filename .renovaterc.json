{
  "extends": ["config:base"],
  "regexManagers": [
    {
      "fileMatch": ["werf\\.yaml$"],
      "matchStrings": [
        "from:\\s*(?<depName>[^:]+)(:(?<currentValue>[^@]+))?"
      ],
      "depNameTemplate": "{{depName}}",
      "datasourceTemplate": "docker",
      "versioningTemplate": "docker",
      "extractVersionTemplate": "{{{currentValue}}}@{{{digest}}}",
      "updateTypes": ["digest"]
    }
  ]
}
